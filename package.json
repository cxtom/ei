{
  "name": "ei",
  "version": "1.2.2",
  "main": "lib/main.js",
  "scripts": {
    "lint": "./node_modules/.bin/fecs src",
    "coverage": "JASMINE_CONFIG_PATH=test/support/jasmine.json ./node_modules/.bin/istanbul cover ./node_modules/.bin/jasmine",
    "coveralls": "JASMINE_CONFIG_PATH=test/support/jasmine.json ./node_modules/.bin/istanbul cover ./node_modules/.bin/jasmine && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "test": "JASMINE_CONFIG_PATH=test/support/jasmine.json ./node_modules/.bin/jasmine",
    "build:amd": "sh tool/build-amd.sh",
    "build:commonjs": "sh tool/build-commonjs.sh",
    "build": "npm run build:amd && npm run build:commonjs",
    "precommit": "npm run lint && npm test",
    "watch": "npm-watch"
  },
  "author": {
    "name": "ludafa@outlook.com"
  },
  "repository": "https://github.com/jinzhubaofu/ei",
  "license": "ISC",
  "peerDependencies": {
    "react": "^0.14.3",
    "react-dom": "^0.14.3"
  },
  "dependencies": {
    "es6-promise": "^2.3.0"
  },
  "readme": "readme.md",
  "devDependencies": {
    "babel-core": "^5.8.34",
    "babel-plugin-external-helpers-2": "^6.3.13",
    "babel-plugin-transform-es2015-block-scoped-functions": "^6.3.13",
    "babel-plugin-transform-es2015-block-scoping": "^6.3.13",
    "babel-plugin-transform-es2015-destructuring": "^6.3.15",
    "babel-plugin-transform-es2015-parameters": "^6.3.13",
    "babel-plugin-transform-es2015-spread": "^6.3.14",
    "babel-plugin-transform-es3-member-expression-literals": "^6.3.13",
    "babel-plugin-transform-es3-property-literals": "^6.3.13",
    "babel-plugin-transform-object-rest-spread": "^6.3.13",
    "babel-plugin-transform-undefined-to-void": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "coveralls": "^2.11.3",
    "expect": "^1.13.0",
    "fecs": "^0.4.16",
    "isparta": "^4.0.0",
    "istanbul": "^0.4.1",
    "jasmine": "^2.3.2",
    "mocha": "^2.3.4",
    "npm-watch": "0.0.1",
    "pre-commit": "^1.1.2",
    "underscore": "^1.8.3"
  },
  "watch": {
    "build:amd": "{src}/*.js"
  }
}
